        </activity>
        <activity android:name=".RegistroUsuarioActivity" />
        <activity
            android:name=".OpcionesUsuarioActivity"
            android:label="@string/title_activity_opciones_usuario"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".TipoCitaActivity" />
        <activity android:name=".AdministradorActivity" />
